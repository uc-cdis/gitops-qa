{
  "notes": [
    "This is the root for niaid data commons qa",
    "works with qa-tb (tb), qa-aids (aids) , qa-microbiome and qa-flu"
  ],
  "versions": {
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "arborist": "quay.io/cdis/arborist:2020.03",
    "fence": "quay.io/cdis/fence:2020.03",
    "indexd": "quay.io/cdis/indexd:2020.03",
    "portal": "quay.io/cdis/data-ecosystem-portal:1.2.4",
    "revproxy": "quay.io/cdis/nginx:1.17.6-ctds-1.0.1",
    "dashboard": "quay.io/cdis/gen3-statics:master",
    "wts": "quay.io/cdis/workspace-token-service:2020.03",
    "manifestservice": "quay.io/cdis/manifestservice:2020.03",
    "hatchery": "quay.io/cdis/hatchery:2020.03",
    "ambassador": "quay.io/datawire/ambassador:0.60.3",
    "guppy": "quay.io/cdis/guppy:2020.03",
    "sheepdog": "quay.io/cdis/sheepdog:2020.03",
    "peregrine": "quay.io/cdis/peregrine:2020.03",
    "pidgin": "quay.io/cdis/pidgin:1.1.0"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "global": {
    "dispatcher_job_num": "10",
    "environment": "qaplanetv1",
    "hostname": "qa-nde.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/ndhdictionary/3.5.3/schema.json",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube_bucket.devplanetv1.gen3",
    "logs_bucket": "logs-devplanetv1-gen3",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "origins_allow_credentials": [
      "https://qa-tb.planx-pla.net",
      "https://qa-aids.planx-pla.net",
      "https://qa-microbiome.planx-pla.net",
      "https://qa-flu.planx-pla.net"
    ],
    "netpolicy": "off",
    "cookie_domain": "planx-pla.net",
    "public_datasets": true,
    "tier_access_level": "libre"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:master"
    }
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "qa_dataset_browser",
        "type": "dataset"
      },
      {
        "index": "qa_data_explorer",
        "type": "subject"
      }
    ],
    "auth_filter_field": "auth_resource_path",
    "enable_encrypt_whitelist": false
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto"
    },
    "fence": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto"
    },
    "indexd": {
      "strategy": "auto"
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "guppy": {
      "strategy": "auto"
    }
  }
}
