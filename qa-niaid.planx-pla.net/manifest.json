{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2020.02",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "datasim": "quay.io/cdis/data-simulator:master",
    "fence": "quay.io/cdis/fence:2020.02",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "indexd": "quay.io/cdis/indexd:2020.02",
    "jupyterhub": "quay.io/occ_data/jupyterhub:master",
    "manifestservice": "quay.io/cdis/manifestservice:2020.02",
    "peregrine": "quay.io/cdis/peregrine:2020.02",
    "pidgin": "quay.io/cdis/pidgin:2020.02",
    "portal": "quay.io/cdis/data-portal:2020.02",
    "revproxy": "quay.io/cdis/nginx:1.17.6-ctds-1.0.1",
    "sheepdog": "quay.io/cdis/sheepdog:2020.02",
    "spark": "quay.io/cdis/gen3-spark:2020.02",
    "tube": "quay.io/cdis/tube:2020.02",
    "wts": "quay.io/cdis/workspace-token-service:2020.02",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:2020.02",
    "guppy": "quay.io/cdis/guppy:2020.02",
    "dashboard": "quay.io/cdis/gen3-statics:master"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "jupyterhub": {
    "enabled": "yes",
    "containers": [
      {
        "name": "niaid-jupyter-notebook",
        "cpu": 1,
        "memory": "512M",
        "image": "quay.io/cdis/niaid-jupyterhub:latest"
      },
      {
        "name": "jupyter-R-3.6",
        "cpu": 1,
        "memory": "512M",
        "image": "quay.io/occ_data/jupyternotebook:1.8.0"
      },
      {
        "name": "jupyter-R-3.5",
        "cpu": 1,
        "memory": "512M",
        "image": "quay.io/occ_data/jupyternotebook:1.7.4"
      }
    ]
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:master"
    }
  },
  "global": {
    "environment": "qaplanetv1",
    "hostname": "qa-niaid.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/ndhdictionary/3.5.4/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-qaplanetv1-gen3",
    "logs_bucket": "logs-qaplanetv1-gen3",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "dispatcher_job_num": "10",
    "netpolicy": "on"
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "qa-niaid",
        "type": "subject"
      },
      {
        "index": "qa-niaid_follow_up",
        "type": "follow_up"
      },
      {
        "index": "qa-niaid_file",
        "type": "file"
      }
    ],
    "config_index": "qa-niaid_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto"
    },
    "fence": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto"
    },
    "indexd": {
      "strategy": "auto"
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "guppy": {
      "strategy": "auto"
    }
  }
}
