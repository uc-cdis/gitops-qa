{
  "notes": [
    "This is the root for niaid data commons qa",
    "works with qa-test2 (tb) and qa-test4 (charlie or whatever)"
  ],
  "versions": {
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "arborist": "quay.io/cdis/arborist:master",
    "fence": "quay.io/cdis/fence:master",
    "indexd": "quay.io/cdis/indexd:master",
    "guppy": "quay.io/cdis/guppy:master",
    "peregrine": "quay.io/cdis/peregrine:master",
    "pidgin": "quay.io/cdis/pidgin:master",
    "portal": "quay.io/cdis/data-ecosystem-portal:master",
    "revproxy": "quay.io/cdis/nginx:1.15.5-ctds",
    "sheepdog": "quay.io/cdis/sheepdog:master",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:master"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "global": {
    "dispatcher_job_num": "10",
    "environment": "qaplanetv1",
    "hostname": "qa-test3.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/ndhdictionary/3.5.3/schema.json",
    "portal_app": "ndef-qa",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube_bucket.devplanetv1.gen3",
    "logs_bucket": "logs-devplanetv1-gen3",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "origins_allow_credentials": ["https://qa-test2.planx-pla.net", "https://qa-test4.planx-pla.net", "https://qa-flu.planx-pla.net"],
    "netpolicy": "off",
    "cookie_domain": "planx-pla.net",
    "public_datasets": true,
    "tier_access_level": "libre"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:master"
    }
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "qa_dataset_browser",
        "type": "dataset"
      },
      {
        "index": "qa_data_explorer",
        "type": "subject"
      }
    ],
    "auth_filter_field": "auth_resource_path",
    "enable_encrypt_whitelist": false
  }
}