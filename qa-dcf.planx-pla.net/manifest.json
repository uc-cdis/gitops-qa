{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "arborist": "quay.io/cdis/arborist:2021.11",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:0.8",
    "datareplicate": "quay.io/cdis/dcf-dataservice:2021.11",
    "fence": "quay.io/cdis/fence:2021.11",
    "indexd": "quay.io/cdis/indexd:2021.11",
    "peregrine": "quay.io/cdis/peregrine:2021.11",
    "pidgin": "quay.io/cdis/pidgin:2021.11",
    "revproxy": "quay.io/cdis/nginx:2021.11",
    "sheepdog": "quay.io/cdis/sheepdog:2021.11",
    "portal": "quay.io/cdis/data-portal:2021.11",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "sower": "quay.io/cdis/sower:2021.11",
    "metadata": "quay.io/cdis/metadata-service:2021.11",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:2021.11",
    "guppy": "quay.io/cdis/guppy:2021.11"
  },
  "sower": [
    {
      "name": "manifest-indexing",
      "action": "index-object-manifest",
      "serviceAccountName": "jobs-qa-dcf-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/manifest-indexing:2021.11",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "serviceAccountName": "jobs-qa-dcf-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/download-indexd-manifest:2021.11",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "pelican-export",
      "action": "export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican-export:2021.11",
        "pull_policy": "Always",
        "env": [
          {
            "name": "DICTIONARY_URL",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "dictionary_url"
              }
            }
          },
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          },
          {
            "name": "ROOT_NODE",
            "value": "subject"
          }
        ],
        "volumeMounts": [
          {
            "name": "pelican-creds-volume",
            "readOnly": true,
            "mountPath": "/pelican-creds.json",
            "subPath": "config.json"
          },
          {
            "name": "peregrine-creds-volume",
            "readOnly": true,
            "mountPath": "/peregrine-creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "4Gi"
      },
      "volumes": [
        {
          "name": "pelican-creds-volume",
          "secret": {
            "secretName": "pelicanservice-g3auto"
          }
        },
        {
          "name": "peregrine-creds-volume",
          "secret": {
            "secretName": "peregrine-creds"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "peregrine": {
    "sidecar": "False"
  },
  "google": {
    "enabled": "yes"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "jupyterhub": {
    "enabled": "no"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:2021.11"
    }
  },
  "guppy": {
    "indices": [
      {
        "index": "qa-dcf_etl",
        "type": "subject"
      },
      {
        "index": "qa-dcf_file",
        "type": "file"
      }
    ],
    "config_index": "qa-dcf_array-config",
    "auth_filter_field": "auth_resource_path"
  },
  "global": {
    "environment": "dcfqav1",
    "hostname": "ctds-test-env.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:733512436101:certificate/2137c4c4-3513-4f89-b763-7b6edf2f229f",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/datadictionary/develop/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-qaplanetv1-gen3",
    "logs_bucket": "logs-qaplanetv1-gen3",
    "sync_from_dbgap": "False",
    "useryaml_s3path": "s3://cdis-gen3-users/qa/user.yaml",
    "dispatcher_job_num": "10",
    "netpolicy": "on",
    "tier_access_level": "regular",
    "tier_access_limit": 50,
    "public_datasets": true,
    "dd_enabled": true
  },
  "canary": {
    "default": 0
  },
  "scaling": {
    "arborist": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "portal": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "fence": {
      "strategy": "auto",
      "min": 5,
      "max": 15,
      "targetCpu": 40
    },
    "presigned-url-fence": {
      "strategy": "auto",
      "min": 20,
      "max": 25,
      "targetCpu": 14
    },
    "indexd": {
      "strategy": "auto",
      "min": 4,
      "max": 8,
      "targetCpu": 40
    },
    "peregrine": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "revproxy": {
      "strategy": "auto",
      "min": 4,
      "max": 10,
      "targetCpu": 40
    },
    "sheepdog": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "guppy": {
      "strategy": "auto",
      "min": 2,
      "max": 4,
      "targetCpu": 40
    },
    "metadata": {
      "strategy": "auto",
      "min": 2,
      "max": 5,
      "targetCpu": 40
    }
  }
}
