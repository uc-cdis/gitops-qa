{
  "user-namespace": "jupyter-pods-qa-anvil",
  "sub-dir": "/lw-workspace",
  "user-volume-size": "500Gi",
  "sidecar": {
    "cpu-limit": "1.0",
    "memory-limit": "512Mi",
    "image": "quay.io/cdis/gen3fuse-sidecar:integration202010",
    "env": {
      "NAMESPACE": "qa-anvil",
      "HOSTNAME": "qa-anvil.planx-pla.net"
    },
    "args": [],
    "command": [
      "/bin/bash",
      "/sidecarDockerrun.sh"
    ],
    "lifecycle-pre-stop": [
      "su",
      "-c",
      "echo test",
      "-s",
      "/bin/sh",
      "root"
    ]
  },
  "containers": [
    {
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "512Mi",
      "name": "R Studio",
      "image": "heliumdatastage/rstudio-server:1",
      "args": [],
      "path-rewrite": "/",
      "use-tls": "false",
      "ready-probe": "/"
    }
  ]
}
