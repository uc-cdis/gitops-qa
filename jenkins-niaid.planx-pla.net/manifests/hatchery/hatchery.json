{
  "user-namespace": "jupyter-pods-jenkins-niaid",
  "sub-dir": "/lw-workspace",
  "user-volume-size": "2Gi",
  "sidecar": {
    "cpu-limit": "1.0",
    "memory-limit": "256Mi",
    "image": "quay.io/cdis/ecs-ws-sidecar:master",
    "env": {
      "NAMESPACE": "jenkins-niaid",
      "HOSTNAME": "jenkins-niaid.planx-pla.net"
    },
    "args": [],
    "command": [
      "/bin/bash",
      "./sidecar.sh"
    ]
  },
  "containers": [
    {
      "target-port": 8787,
      "cpu-limit": "1.0",
      "memory-limit": "2Gi",
      "name": "R Studio",
      "image": "heliumdatastage/rstudio-server:1",
      "env": {
        "DISABLE_AUTH": "true"
      },
      "args": [],
      "path-rewrite": "/",
      "use-tls": "false",
      "ready-probe": "/"
    },
    {
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "2Gi",
      "name": "Jupyter Notebook Bio Python",
      "image": "quay.io/occ_data/jupyternotebook:1.7.2",
      "env": {
        "NAMESPACE": "jenkins-niaid"
      },
      "args": [
        "--NotebookApp.base_url=/lw-workspace/proxy/",
        "--NotebookApp.password=''",
        "--NotebookApp.token=''",
        "--NotebookApp.quit_button=False"
      ],
      "command": [
        "start-notebook.sh"
      ],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "lifecycle-post-start": [
        "/bin/sh",
        "-c",
        "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; true"
      ],
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/home/jovyan/pd"
    },
    {
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "2Gi",
      "name": "(Tutorial) Bacpac Synthetic Data Analysis Notebook",
      "image": "quay.io/cdis/heal-notebooks:bacpac__10670ad79b4488cfa9f1b3681ce52bc3f1139b5a",
      "env": {
        "FRAME_ANCESTORS": "https://jenkins-niaid.planx-pla.net"
      },
      "args": [
        "--NotebookApp.base_url=/lw-workspace/proxy/",
        "--NotebookApp.password=''",
        "--NotebookApp.token=''",
        "--NotebookApp.quit_button=False"
      ],
      "command": [
        "start-notebook.sh"
      ],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "lifecycle-post-start": [
        "/bin/sh",
        "-c",
        "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; rm -rf /home/$IAM/pd/lost+found; ln -s /data /home/$IAM/; true"
      ],
      "user-uid": 1000,
      "fs-gid": 100    }
  ]
}
