{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "versions": {
    "arborist": "quay.io/cdis/arborist:2021.03",
    "aws-es-proxy": "abutaha/aws-es-proxy:0.8",
    "covid19-etl": "quay.io/cdis/covid19-etl:4.6.5",
    "nb-etl": "quay.io/cdis/covid19-nb-etl:4.6.7",
    "fence": "quay.io/cdis/fence:2021.03",
    "indexd": "quay.io/cdis/indexd:2021.03",
    "peregrine": "quay.io/cdis/peregrine:2021.03",
    "pidgin": "quay.io/cdis/pidgin:2021.03",
    "revproxy": "quay.io/cdis/nginx:2021.03",
    "sheepdog": "quay.io/cdis/sheepdog:2021.03",
    "portal": "quay.io/cdis/data-portal:2.47.1",
    "tube": "quay.io/cdis/tube:2021.03",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.2-debian-cloudwatch",
    "spark": "quay.io/cdis/gen3-spark:2021.03",
    "hatchery": "quay.io/cdis/hatchery:2021.03",
    "mariner": "quay.io/cdis/mariner-server:2021.03",
    "wts": "quay.io/cdis/workspace-token-service:2021.03",
    "ssjdispatcher": "quay.io/cdis/ssjdispatcher:2021.03",
    "ambassador": "quay.io/datawire/ambassador:1.4.2",
    "guppy": "quay.io/cdis/guppy:2021.03",
    "manifestservice": "quay.io/cdis/manifestservice:2021.03",
    "dashboard": "quay.io/cdis/gen3-statics:2021.03",
    "auspice": "quay.io/cdis/gen3-auspice:v2.16.gen3.11",
    "sower": "quay.io/cdis/sower:2021.03",
    "metadata": "quay.io/cdis/metadata-service:2021.03"
  },
  "global": {
    "environment": "qa-covid19",
    "hostname": "qa-covid19.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/covid19-datadictionary/3.8.1/schema.json",
    "portal_app": "gitops",
    "kube_bucket": "kube-qaplanetv2-gen3",
    "logs_bucket": "logs-qaplanetv2-gen3",
    "sync_from_dbgap": "False",
    "dispatcher_job_num": "10",
    "netpolicy": "off",
    "public_datasets": true,
    "tier_access_level": "regular",
    "tier_access_limit": "2",
    "useryaml_s3path": "s3://cdis-gen3-users/qa-covid19/user.yaml",
    "maintenance_mode": "off"
  },
  "portal": {
    "GEN3_BUNDLE": "covid19"
  },
  "canary": {
    "default": 0
  },
  "indexd": {
    "arborist": "true"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "ssjdispatcher": {
    "job_images": {
      "indexing": "quay.io/cdis/indexs3client:2021.03"
    }
  },
  "sower" : [
    {
      "name": "manifest-indexing",
      "action": "index-object-manifest",
      "serviceAccountName": "jobs-qa-covid19-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/manifest-indexing:2021.03",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    },
    {
      "name": "indexd-manifest",
      "action": "download-indexd-manifest",
      "serviceAccountName": "jobs-qa-covid19-planx-pla-net",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/download-indexd-manifest:2021.03",
        "pull_policy": "Always",
        "env": [
          {
            "name": "GEN3_HOSTNAME",
            "valueFrom": {
              "configMapKeyRef": {
                "name": "manifest-global",
                "key": "hostname"
              }
            }
          }
        ],
        "volumeMounts": [
          {
            "name": "sower-jobs-creds-volume",
            "readOnly": true,
            "mountPath": "/creds.json",
            "subPath": "creds.json"
          }
        ],
        "cpu-limit": "1",
        "memory-limit": "1Gi"
      },
      "volumes": [
        {
          "name": "sower-jobs-creds-volume",
          "secret": {
            "secretName": "sower-jobs-g3auto"
          }
        }
      ],
      "restart_policy": "Never"
    }
  ],
  "guppy": {
    "indices": [
      {
        "index": "qa-covid19",
        "type": "location"
      },
      {
        "index": "qa-covid19_genomic_file",
        "type": "genomic_file"
      },
      {
        "index": "qa-covid19_subject",
        "type": "subject"
      },
      {
        "index": "qa-covid19_clinical_trials",
        "type": "clinical_trials"
      },
      {
        "index": "qa-covid19_dataset",
        "type": "dataset"
      }
    ],
    "config_index": "qa-covid19_array-config",
    "auth_filter_field": "auth_resource_path",
    "aggs_include_missing_data": false
  },
  "mariner": {
    "storage": {
      "s3": {
        "name": "workflow-engine-garvin",
        "region": "us-east-1"
      }
    },
    "secrets": {
      "awsusercreds": {
        "name": "workflow-bot-g3auto",
        "key": "awsusercreds.json"
      }
    },
    "jobs": {
      "engine": {
        "labels": {
          "app": "mariner-engine"
        },
        "serviceaccount": "mariner-service-account",
        "restart_policy": "never"
      },
      "task": {
        "labels": {
          "app": "mariner-task"
        },
        "restart_policy": "never"
      }
    },
    "containers": {
      "engine": {
        "name": "mariner-engine",
        "image": "quay.io/cdis/mariner-engine:2021.03",
        "pull_policy": "always",
        "command": [
          "/mariner",
          "run",
          "$(RUN_ID)"
        ],
        "resources": {
          "limits": {
            "memory": "8Gi",
            "cpu": "800m"
          }
        }
      },
      "s3sidecar": {
        "name": "mariner-s3sidecar",
        "image": "quay.io/cdis/mariner-s3sidecar:2021.03",
        "pull_policy": "always",
        "command": [
          "/sidecar"
        ],
        "resources": {
          "limits": {
            "memory": "4Gi",
            "cpu": "500m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "gen3fusesidecar": {
        "name": "mariner-gen3fusesidecar",
        "image": "quay.io/cdis/gen3fuse-sidecar:2021.03",
        "pull_policy": "always",
        "command": [
          "/bin/bash",
          "/marinerRun.sh"
        ],
        "lifecycle": {
          "prestop": [
            "/bin/bash",
            "-c",
            "fusermount -u -z /$COMMONS_DATA"
          ]
        },
        "resources": {
          "limits": {
            "memory": "256Mi",
            "cpu": "100m"
          }
        },
        "securitycontext": {
          "privileged": true
        }
      },
      "task": {
        "name": "mariner-task",
        "image": "alpine",
        "pull_policy": "always",
        "command": [
          "/bin/sh"
        ],
        "resources": {
          "limits": {
            "memory": "512Mi",
            "cpu": "900m"
          }
        }
      }
    }
  }
}
