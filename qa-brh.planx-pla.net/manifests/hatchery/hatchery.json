      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/home/jovyan/pd",
      "gen3-volume-location": "/home/jovyan/.gen3",
      "authz": {
        "version": 0.1,
        "or": [
          {
            "resource_paths": [
              "/workspace/jupyter-nextflow"
            ]
          },
          {
            "pay_models": [
              "Direct Pay"
            ]
          }
        ]
      },
      "nextflow": {
