{
	"configuration": {
		"schema": {
			"_subjects_count": {
				"type": "integer"
			},
			"__manifest": {
				"type": "array",
				"properties": {
					"file_name": {
						"type": "string"
					},
					"file_size": {
						"type": "integer"
					}
				}
			},
			"tags": {
				"type": "array"
			},
			"_unique_id": {},
			"study_description": {},
			"study_id": {},
			"study_url": {},
			"project_id": {},
			"short_name": {},
			"full_name": {},
			"commons_url": {},
			"commons" : {},
			"authz": {
				"type": "string"
			}
		},
		"settings" : {
			"cache_drs" : true
		}
	},
	"adapter_commons": {
