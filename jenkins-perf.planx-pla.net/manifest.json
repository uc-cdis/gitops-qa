{
  "notes": [
    "This is the dev environment manifest",
    "That's all I have to say"
  ],
  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2024.10",
    "aws-es-proxy": "quay.io/cdis/aws-es-proxy:v1.3.1",
    "fence": "quay.io/cdis/fence:2024.10",
    "fluentd": "fluent/fluentd-kubernetes-daemonset:v1.15.3-debian-cloudwatch-1.0",
    "indexd": "quay.io/cdis/indexd:2024.10",
    "peregrine": "quay.io/cdis/peregrine:2024.10",
    "portal": "quay.io/cdis/data-portal:2024.10",
    "revproxy": "quay.io/cdis/nginx:2024.10",
    "sheepdog": "quay.io/cdis/sheepdog:2024.10",
    "manifestservice": "quay.io/cdis/manifestservice:2024.10",
    "wts": "quay.io/cdis/workspace-token-service:2024.10",
    "sower": "quay.io/cdis/sower:2024.10",
    "hatchery": "quay.io/cdis/hatchery:2024.10",
    "metadata": "quay.io/cdis/metadata-service:2024.10",
    "dashboard": "quay.io/cdis/gen3-statics:2024.10"
  },
  "arborist": {
    "deployment_version": "2"
  },
  "indexd": {
    "arborist": "true"
  },
  "arranger": {
    "project_id": "dev",
    "auth_filter_field": "gen3_resource_path",
    "auth_filter_node_types": [
      "subject"
    ]
  },
  "metadata": {
    "USE_AGG_MDS": true,
    "AGG_MDS_NAMESPACE": "jenkins-perf"
  },
  "sower": [
    {
      "name": "pelican-export",
      "container": {
        "name": "job-task",
        "image": "quay.io/cdis/pelican:2024.10",
        "pull_policy": "always",
        "env": []
      },
      "restart_policy": "never"
    }
  ],
  "peregrine": {
    "sidecar": "True"
  },
  "global": {
    "environment": "qaplanetv1",
    "hostname": "jenkins-perf.planx-pla.net",
    "revproxy_arn": "arn:aws:acm:us-east-1:707767160287:certificate/c676c81c-9546-4e9a-9a72-725dd3912bc8",
    "dictionary_url": "https://s3.amazonaws.com/dictionary-artifacts/gtexdictionary/4.4.0/schema.json",
    "portal_app": "gitops",
    "sync_from_dbgap": "False",
    "kube_bucket": "kube_bucket.devplanetv1.gen3",
    "logs_bucket": "logs-devplanetv1-gen3",
    "useryaml_s3path": "s3://cdis-gen3-users/ci/user.yaml",
    "es7": true
  },
  "canary": {
    "default": 0
  },
  "guppy": {
    "indices": [
      {
        "index": "jenkins_subject_alias",
        "type": "subject"
      },
      {
        "index": "jenkins_file_alias",
        "type": "file"
      },
      {
        "index": "jenkins_midrc_dataset_alias",
        "type": "dataset"
      },
      {
        "index": "jenkins_midrc_case_alias",
        "type": "case"
      },
      {
        "index": "jenkins_midrc_imaging_data_file_alias",
        "type": "imaging_data_file"
      },
      {
        "index": "jenkins_midrc_imaging_study_alias",
        "type": "imaging_study"
      }
    ],
    "config_index": "jenkins_configs_alias",
    "auth_filter_field": "auth_resource_path"
  },
  "scaling": {
    "arborist": {
      "strategy": "auto"
    },
    "portal": {
      "strategy": "auto",
      "min": 1,
      "max": 2
    },
    "fence": {
      "strategy": "auto"
    },
    "presigned-url-fence": {
      "strategy": "auto"
    },
    "indexd": {
      "strategy": "auto"
    },
    "peregrine": {
      "strategy": "auto"
    },
    "revproxy": {
      "strategy": "auto"
    },
    "sheepdog": {
      "strategy": "auto"
    },
    "guppy": {
      "strategy": "auto"
    }
  }
}