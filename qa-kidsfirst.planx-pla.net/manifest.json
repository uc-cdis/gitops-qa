  "jenkins": {
    "autodeploy": "yes"
  },
  "versions": {
    "arborist": "quay.io/cdis/arborist:2.3.2",
    "dashboard": "quay.io/cdis/gen3-statics:1.0.0",
    "fence": "quay.io/cdis/fence:4.8.0",
    "indexd": "quay.io/cdis/indexd:2.5.0",
