{
  "user-namespace": "jupyter-pods-qa-mickey",
  "sub-dir": "/lw-workspace",
  "user-volume-size": "10Gi",
  "sidecar": {
    "cpu-limit": "1.0",
    "memory-limit": "1Gi",
    "image": "quay.io/cdis/gen3fuse-sidecar:2020.08",
    "env": {
      "NAMESPACE": "qa-mickey",
      "HOSTNAME": "qa-mickey.planx-pla.net"
    },
    "args": [],
    "command": [
      "/bin/bash",
      "/sidecarDockerrun.sh"
    ],
    "lifecycle-pre-stop": [
      "su",
      "-c",
      "echo test",
      "-s",
      "/bin/sh",
      "root"
    ]
  },
  "containers": [
    {
      "target-port": 8787,
      "cpu-limit": "1.0",
      "memory-limit": "512Mi",
      "name": "R Studio",
      "image": "heliumdatastage/rstudio-server:1",
      "env": {
        "DISABLE_AUTH": "true"
      },
      "args": [],
      "path-rewrite": "/",
      "use-tls": "false",
      "ready-probe": "/"
    },
    {
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "512Mi",
      "name": "Jupyter Notebook Bio Python",
      "image": "quay.io/cdis/jupyter-notebook:1.0.2",
      "env": {},
      "args": [
        "--NotebookApp.base_url=/lw-workspace/proxy/",
        "--NotebookApp.password=''",
        "--NotebookApp.token=''"
      ],
      "command": [
        "start-notebook.sh"
      ],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "lifecycle-post-start": [
        "/bin/sh",
        "-c",
        "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
      ],
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/home/jovyan/pd"
    },
    {
      "target-port": 8888,
      "cpu-limit": "1.0",
      "memory-limit": "512Mi",
      "name": "Jupyter Notebook Bio Python (Lab Edition)",
      "image": "quay.io/cdis/jupyter-notebook:1.0.2",
      "env": {},
      "args": [
        "--NotebookApp.base_url=/lw-workspace/proxy/",
        "--NotebookApp.default_url=/lab",
        "--NotebookApp.password=''",
        "--NotebookApp.token=''"
      ],
      "command": [
        "start-notebook.sh"
      ],
      "path-rewrite": "/lw-workspace/proxy/",
      "use-tls": "false",
      "ready-probe": "/lw-workspace/proxy/",
      "lifecycle-post-start": [
        "/bin/sh",
        "-c",
        "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
      ],
      "user-uid": 1000,
      "fs-gid": 100,
      "user-volume-location": "/home/jovyan/pd"
    },
    {
      "target-port": 5800,
      "cpu-limit": "1.0",
      "memory-limit": "1024Mi",
      "name": "R Studio thru VNC",
      "image": "heliumdatastage/rstudio-server:1",
      "env": {
        "DISABLE_AUTH": "true"
      },
      "args": [],
      "path-rewrite": "/",
      "use-tls": "false",
      "ready-probe": "/",
      "use-shared-memory": "true",
      "friends": [
        {
          "name": "firefox",
          "image": "quay.io/cdis/docker-firefox:master",
          "securityContext": {
            "privileged": true
          },
          "env": [
            {
              "name": "LANDING_URL",
              "value": "http://localhost:8787/"
            }
          ],
          "resources": {
            "limits": {
              "cpu": "1.0",
              "memory": "5Gi"
            }
          },
          "ports": [
            {
              "containerPort": 5800
            }
          ]
        }
      ]
    },
    {
      "target-port": 5800,
      "cpu-limit": "1.0",
      "memory-limit": "512Mi",
      "name": "Jupyter Notebook Bio Python thru VNC",
      "image": "quay.io/cdis/jupyter-notebook:1.0.2",
      "env": {},
      "args": [
        "--NotebookApp.base_url=/lw-workspace/proxy/",
        "--NotebookApp.password=''",
        "--NotebookApp.token=''"
      ],
      "command": [
        "start-notebook.sh"
      ],
      "path-rewrite": "/",
      "use-tls": "false",
      "ready-probe": "/",
      "lifecycle-post-start": [
        "/bin/sh",
        "-c",
        "export IAM=`whoami`; rm -rf /home/$IAM/pd/dockerHome; ln -s $(pwd) /home/$IAM/pd/dockerHome; mkdir -p /home/$IAM/.jupyter/custom; echo \"define(['base/js/namespace'], function(Jupyter){Jupyter._target = '_self';})\" >/home/$IAM/.jupyter/custom/custom.js; ln -s /data /home/$IAM/pd/; true"
      ],
      "use-shared-memory": "true",
      "friends": [
        {
          "name": "firefox",
          "image": "quay.io/cdis/docker-firefox:master",
          "securityContext": {
            "privileged": true
          },
          "env": [
            {
              "name": "LANDING_URL",
              "value": "http://localhost:8888/lw-workspace/proxy/"
            }
          ],
          "resources": {
            "limits": {
              "cpu": "1.0",
              "memory": "5Gi"
            }
          },
          "ports": [
            {
              "containerPort": 5800
            }
          ]
        }
      ]
    }
  ]
}
